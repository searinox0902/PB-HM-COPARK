        mc:Ignorable="d"
        Title="Administrarusuarioseliminar" Height="450" Width="800">
    <Grid>
        <Button x:Name="btneliminar" Content="ELIMINAR" HorizontalAlignment="Left" Margin="626,351,0,0" VerticalAlignment="Top" Width="75" Click="btneliminar_Click"/>
