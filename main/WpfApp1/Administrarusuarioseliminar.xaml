        <Image HorizontalAlignment="Left" Height="25" Margin="626,10,0,0" VerticalAlignment="Top" Width="137" Source="main-logo-white.png" RenderTransformOrigin="-0.551,0.54"/>
        <Label x:Name="lbstex" Content="" HorizontalAlignment="Left" Margin="22,82,0,0" VerticalAlignment="Top" Width="130" Background="#FFB4CDB0"/>
        <ListBox HorizontalAlignment="Left" Height="206" Margin="52,148,0,0" VerticalAlignment="Top" Width="168"/>
        <Rectangle HorizontalAlignment="Left" Height="180" Margin="308,134,0,0" VerticalAlignment="Top" Width="410" Fill="White"/>
